{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"id": "http://jsonschema.net",
	"type": "object",
	"properties": {
		"areaName": {
			"type": "string",
			"maxLength": 6
		},
		"cityName": {
			"type": "number",
			"maximum": 100000,
			"pattern": "^[0-9]{1,5}(\\.[0-9]{1,2})?$"
		}
	},
	"required": [
		"areaName","cityName"
	]
}